{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/rss",
      "destination": "/rss.xml"
    },
    {
      "source": "/feed",
      "destination": "/rss.xml"
    },
    {
      "source": "/feed/",
      "destination": "/rss.xml"
    }
  ]
}